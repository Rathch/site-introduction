{
    "name": "bk2k/introduction-website",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Benjamin Kott",
            "email": "benjamin.kott@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "repositories": [
        {
            "type": "path",
            "url": "local_packages/*"
        },
        {
            "type": "composer",
            "url": "https://composer.typo3.org/"
        }
    ],
    "require": {
        "php": "^7.4.1 || ^8",
        "bk2k/bootstrap-package": "^12.0.0 || ^13.0.0",
        "friendsoftypo3/feedit": "dev-11-12",
        "friendsoftypo3/sys-action": "dev-master",
        "typo3/cms-adminpanel": "^11.5.23",
        "typo3/cms-backend": "^11.5.23",
        "typo3/cms-belog": "^11.5.23",
        "typo3/cms-beuser": "^11.5.23",
        "typo3/cms-core": "^11.5.23",
        "typo3/cms-dashboard": "^11.5.23",
        "typo3/cms-extbase": "^11.5.23",
        "typo3/cms-extensionmanager": "^11.5.23",
        "typo3/cms-felogin": "^11.5.23",
        "typo3/cms-filelist": "^11.5.23",
        "typo3/cms-filemetadata": "^11.5.23",
        "typo3/cms-fluid": "^11.5.23",
        "typo3/cms-form": "^11.5.23",
        "typo3/cms-frontend": "^11.5.23",
        "typo3/cms-impexp": "^11.5.23",
        "typo3/cms-indexed-search": "^11.5.23",
        "typo3/cms-info": "^11.5.23",
        "typo3/cms-install": "^11.5.23",
        "typo3/cms-linkvalidator": "^11.5.23",
        "typo3/cms-lowlevel": "^11.5.23",
        "typo3/cms-opendocs": "^11.5.23",
        "typo3/cms-recordlist": "^11.5.23",
        "typo3/cms-recycler": "^11.5.23",
        "typo3/cms-redirects": "^11.5.23",
        "typo3/cms-reports": "^11.5.23",
        "typo3/cms-rte-ckeditor": "^11.5.23",
        "typo3/cms-scheduler": "^11.5.23",
        "typo3/cms-seo": "^11.5.23",
        "typo3/cms-setup": "^11.5.23",
        "typo3/cms-sys-note": "^11.5.23",
        "typo3/cms-t3editor": "^11.5.23",
        "typo3/cms-tstemplate": "^11.5.23",
        "typo3/cms-viewpage": "^11.5.23",
        "typo3/cms-workspaces": "^11.5.23",
        "vlucas/phpdotenv": "^2.0.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1.22",
        "codeception/module-asserts": "^1.3",
        "codeception/module-webdriver": "^1.4"
    },
    "scripts": {
      "test": [
        "codecept run acceptance -d -c ./Tests/codeception.yml"
      ]
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin",
        "vendor-dir": "vendor",
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
